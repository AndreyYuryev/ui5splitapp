<mvc:View
    controllerName="routingsplitapp.controller.Master"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Page id="page" title="{i18n>title}">
		<content>
            <List items="{/EventSet}">
                <ObjectListItem type="Active" title="{Title}" intro="{Status}" number="{EventDate}" numberUnit="{Status}" press="onItemPress"/>
            </List>
        </content>
	</Page>

</mvc:View>
