{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SplitApp",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": [
        "--open",
        "index.html?sap-client=800"
      ],
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/routingsplitapp/SplitApp\"}",
        "FIORI_TOOLS_UI5_VERSION": "1.95.0",
        "FIORI_TOOLS_UI5_URI": "https://ui5.sap.com"
      }
    }
  ]
}